ifneq ($PSD_TYPE,"")
        PSD_VERSION := $(PSD_VERSION_MAINTENANCE)-$(PSD_VERSION_TAG)-V$(PSD_VERSION_MAJOR).$(PSD_VERSION_MINOR)
else
        PSD_VERSION := $(PSD_VERSION_MAINTENANCE)-V$(PSD_VERSION_MAJOR).$(PSD_VERSION_MINOR)
endif

export BUILD_DATE := $(shell date -u +%Y%m%d)
export PSD_BUILD_NAME := $(PSD_VERSION)-$(BUILD_DATE)

PRODUCT_PROPERTY_OVERRIDES += \
    ro.$(VENDOR_PSD).version=$(PSD_VERSION)
